{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "allowUnreachableCode": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist"
  },
  "lib": ["ESNext"],
  "exclude": ["./output"]
}
